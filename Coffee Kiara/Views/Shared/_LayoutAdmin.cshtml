<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="~/Content/Admin/CSS/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/Admin/CSS/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/Admin/CSS/mmc-chat.css" rel="stylesheet" />
    <link href="~/Content/Admin/CSS/owl.carousel.min.css" rel="stylesheet" />
    <link href="~/Content/Admin/CSS/owl.theme.default.min.css" rel="stylesheet" />
    <link href="~/Content/Admin/CSS/sidebar.css" rel="stylesheet" />
    <link href="~/Content/Admin/CSS/simdahs.css" rel="stylesheet" />
    <link href="~/Content/Admin/CSS/style.css" rel="stylesheet" />
    <link href="~/Content/Admin/CSS/themify-icons.css" rel="stylesheet" />
    <link href="~/Content/Admin/CSS/weather-icons.css" rel="stylesheet" />
    @*@Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")*@
</head>
<body>
    <div class="sidebar sidebar-hide-to-small sidebar-shrink sidebar-gestures">
        <div class="nano">
            <div class="nano-content">
                <ul>
                    <li class="label">Main</li>
                    <li class="active"><a href="@Url.Action("Index", "Admin")"><i class="ti-menu-alt"></i> Menu </a></li>

                    <li><a href="@Url.Action("Profile", "Admin")"><i class="ti-receipt"></i> Order list</a></li>

                    <li><a href="@Url.Action("Logout", "Login")"><i class="ti-close"></i> Logout</a></li>

                </ul>
            </div>
        </div>
    </div><!-- /# sidebar -->




    <div class="header">
        <div class="pull-left">
            <div class="logo" id="sideLogo">
                <a href="index.html">

                </a>
            </div>
            <div class="hamburger sidebar-toggle">
                <span class="ti-menu"></span>
            </div>
        </div>

        <div class="pull-right p-r-15">
            <ul>
                <li class="header-icon dib">
                    <i class="ti-bell"></i>
                    <div class="drop-down">
                        <div class="dropdown-content-heading">
                            <span class="text-left">Recent Notifications</span>
                        </div>
                        <div class="dropdown-content-body">
                            <ul>
                                <li>
                                    <a href="#">
                                        <img class="pull-left m-r-10 avatar-img" src="~/Content/Admin/images/3.jpg" alt="" />
                                        <div class="notification-content">
                                            <small class="notification-timestamp pull-right">02:34 PM</small>
                                            <div class="notification-heading">Mr. Wolf</div>
                                            <div class="notification-text">Order No.1 Masuk </div>
                                        </div>
                                    </a>
                                </li>

                                <li>
                                    <a href="#">
                                        <img class="pull-left m-r-10 avatar-img" src="~/Content/Admin/images/3.jpg" alt="" />
                                        <div class="notification-content">
                                            <small class="notification-timestamp pull-right">02:35 PM</small>
                                            <div class="notification-heading">Mr. Wolf</div>
                                            <div class="notification-text">Order No.2 Masuk </div>
                                        </div>
                                    </a>
                                </li>

                                <li>
                                    <a href="#">
                                        <img class="pull-left m-r-10 avatar-img" src="~/Content/Admin/images/3.jpg" alt="" />
                                        <div class="notification-content">
                                            <small class="notification-timestamp pull-right">02:37 PM</small>
                                            <div class="notification-heading">Mr. Wolf</div>
                                            <div class="notification-text">Order No.3 Masuk </div>
                                        </div>
                                    </a>
                                </li>
                                <li class="text-center">
                                    <a href="#" class="more-link">See All</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </li>
 
                <li class="header-icon dib">
                    <img class="avatar-img" src="~/Content/Admin/images/1.jpg" alt="" /> <span class="user-avatar">David <i class="ti-angle-down f-s-10"></i></span>
                    <div class="drop-down dropdown-profile">
                        <div class="dropdown-content-heading">
                            <span class="text-left">Upgrade Now</span>
                            <p class="trial-day">30 Days Trail</p>
                        </div>
                        <div class="dropdown-content-body">
                            <ul>
                                <li><a href="#"><i class="ti-user"></i> <span>Profile</span></a></li>
                                <li><a href="#"><i class="ti-wallet"></i> <span>My Balance</span></a></li>
                                <li><a href="#"><i class="ti-write"></i> <span>My Task</span></a></li>
                                <li><a href="#"><i class="ti-calendar"></i> <span>My Calender</span></a></li>
                                <li><a href="#"><i class="ti-email"></i> <span>Inbox</span></a></li>
                                <li><a href="#"><i class="ti-settings"></i> <span>Setting</span></a></li>
                                <li><a href="#"><i class="ti-help-alt"></i> <span>Help</span></a></li>
                                <li><a href="#"><i class="ti-lock"></i> <span>Lock Screen</span></a></li>
                                <li><a href="@Url.Action("Logout", "Login")"><i class="ti-power-off"></i> <span>Logout</span></a></li>
                            </ul>
                        </div>
                    </div>
                </li>
            </ul>
        </div>
    </div>
    @RenderBody()
    @*@Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")*@
    @RenderSection("scripts", required: false)
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Content/Admin/Js/jquery.min.js"></script>
    <script src="~/Content/Admin/Js/jquery.nanoscroller.min.js"></script>
    <script src="~/Content/Admin/Js/mmc-chat.js"></script>
    <script src="~/Content/Admin/Js/mmc-common.js"></script>
    <script src="~/Content/Admin/Js/scripts.js"></script>
    <script src="~/Content/Admin/Js/sidebar.js"></script>
    <script src="~/Content/Admin/Js/weather/weather-init.js"></script>
    <script src="~/Content/Admin/Js/chart-js/Chart.bundle.js"></script>
    <script src="~/Content/Admin/Js/chart-js/chartjs-init.js"></script>
    <script src="~/Content/Admin/Js/datamap/d3.min.js"></script>
    <script src="~/Content/Admin/Js/datamap/datamap-init.js"></script>
    <script src="~/Content/Admin/Js/datamap/datamaps.world.min.js"></script>
    <script src="~/Content/Admin/Js/datamap/topojson.js"></script>
    <script src="~/Content/Admin/Js/owl-carousel/owl.carousel-init.js"></script>
    <script src="~/Content/Admin/Js/owl-carousel/owl.carousel.min.js"></script>
    <script src="~/Content/Admin/Js/sparklinechart/jquery.sparkline.min.js"></script>
    <script src="~/Content/Admin/Js/sparklinechart/sparkline.init.js"></script>
    <script src="~/Content/Admin/Js/weather/jquery.simpleWeather.min.js"></script>
</body>
</html>
